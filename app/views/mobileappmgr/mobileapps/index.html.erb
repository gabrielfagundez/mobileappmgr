<table>
  <tr>
    <th>Platform</th>
    <th>Name</th>
    <th>Version</th>
    <th>Uploaded</th>
    <th>&nbsp;</th>
  </tr>

<% @mobileapps.each do |mobileapp| %>
  <tr>
    <td><%= mobileapp['platform']%></td>
    <td><%= mobileapp['name'] %></td>
    <td><%= mobileapp['version'] %></td>
    <td><%= mobileapp['uploaded']%></td>
    <td><a href="<%= mobileapp['install'] %>">Install</a></td>
  </tr>
<% end %>
</table>

<br />

